<div class='ui grid'>
  <div class='column'></div>
    <%= render partial: 'admin/partials/dashboard_side_bar' %>

    <div class='ten wide column'>
      <br>
      <div class='ui breadcrumb row'>
          <%= link_to 'Profile', teacher_dashboard_path(current_user), class:'section' %>
          <span class='divider'>/</span>
          <div class='active section'>Courses</div>
          <table class="ui compact celled definition table">
          <thead>
            <tr>
              <th></th>
              <th>Name</th>
              <th>Registration Date</th>
              <th>E-mail address</th>
              <th>User Type</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody>
            <% @users.each do |user| %>
            <tr>
              <td class="collapsing">
                <div class="ui fitted slider checkbox">
                  <input type="checkbox"> <label></label>
                </div>
              </td>
              <td><%= user.name %></td>
              <td><%= user.created_at.to_date %></td>
              <td><%= user.email %></td>
              <td><%= user.user_type %></td>
              <td></td>
            </tr>
            <% end %>
          </tbody>
          <tfoot class="full-width">
            <tr>
              <th></th>
              <th colspan="5">
                <%= link_to semantic_icon('user icon')+'Add User',new_admin_user_path(@user), class:"ui right floated small primary labeled icon button" %>
              </th>
            </tr>
          </tfoot>
        </table>
      </div>
  </div>
</div>
